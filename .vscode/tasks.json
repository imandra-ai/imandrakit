{
	// See https://go.microsoft.com/fwlink/?LinkId=733558
	// for the documentation about the tasks.json format
	"version": "2.0.0",
	"tasks": [
		{
			"type": "dune",
			"problemMatcher": [
				"$ocamlc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"label": "dune: build /examples/debug/dune",
			"options": {
				"cwd": "${workspaceFolder}/examples/debug"
			},
			"presentation": {
				"reveal": "silent",
			}
		}
	]
}